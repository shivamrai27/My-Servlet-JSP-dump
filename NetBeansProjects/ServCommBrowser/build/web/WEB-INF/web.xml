        <servlet-name>openjavaa</servlet-name>
        <url-pattern>/openjava</url-pattern>
    </servlet-mapping>
    
<!--=========================================================================================-->

    <servlet>
        <servlet-name>opennewjavaa</servlet-name>
        <servlet-class>com.shivam.browserCommunication.OracleJava</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>opennewjavaa</servlet-name>
        <url-pattern>/opennewjava</url-pattern>
